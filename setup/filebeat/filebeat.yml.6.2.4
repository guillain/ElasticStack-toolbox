filebeat.prospectors:
    -
      paths:
        - /var/log/messages
        - /var/log/cron
        - /var/log/secure
        - /var/log/maillog
        - /var/log/firewalld
      input_type: log
      fields_under_root: true
      fields:
            clientip: 54.93.180.49
            type: syslog
    -
      paths:
        - /var/log/audit/audit.log
      input_type: log
      fields_under_root: true
      fields:
            clientip: 54.93.180.49
            type: audit

    -
      paths:
        - /var/log/yum.log
      input_type: log
      fields_under_root: true
      fields:
            clientip: 54.93.180.49
            type: dpkg

    -
      paths:
        - /var/log/elasticsearch/MyELK.log
      input_type: log
      fields_under_root: true
      fields:
            clientip: 54.93.180.49
            type: elasticsearch
